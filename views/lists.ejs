<%- include("header") -%>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    <div class="box">
         <%   for(var i=0;i<newListItem.length;i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%= newListItem[i] %></p>
            </div>
          <%  } %>

          <form class="" action="/" method="post">
            <input type="text" name="newItem" placeholder="NEW ITEM">
            <button type="submit" name="pge" value=<%= listTitle %>>+</button>
        </form>
    </div>

<%- include("footer") -%>